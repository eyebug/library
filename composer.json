{
    "name": "frankli/mylib",
    "description": "My library for Laravel Framework.",
    "keywords": ["framework", "laravel", "customized", "library"],
    "license": "GNU",
    "type": "library",
    "require": {
        "php": ">=7.0.20",
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
